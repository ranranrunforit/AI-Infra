{
    "annotations": {
        "list": []
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "id": null,
    "links": [],
    "panels": [
        {
            "title": "Request Rate",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "targets": [
                {
                    "expr": "rate(model_serving_requests_total[5m])",
                    "legendFormat": "{{model}} - {{status}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps",
                    "color": {
                        "mode": "palette-classic"
                    }
                }
            }
        },
        {
            "title": "Request Latency (P50, P95, P99)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.50, rate(model_serving_request_duration_seconds_bucket[5m]))",
                    "legendFormat": "P50"
                },
                {
                    "expr": "histogram_quantile(0.95, rate(model_serving_request_duration_seconds_bucket[5m]))",
                    "legendFormat": "P95"
                },
                {
                    "expr": "histogram_quantile(0.99, rate(model_serving_request_duration_seconds_bucket[5m]))",
                    "legendFormat": "P99"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "color": {
                        "mode": "palette-classic"
                    }
                }
            }
        },
        {
            "title": "Active Requests",
            "type": "gauge",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 0,
                "y": 8
            },
            "targets": [
                {
                    "expr": "sum(model_serving_active_requests)",
                    "legendFormat": "Active"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 50
                            },
                            {
                                "color": "red",
                                "value": 100
                            }
                        ]
                    },
                    "max": 200
                }
            }
        },
        {
            "title": "Batch Size Distribution",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 6,
                "y": 8
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.50, rate(model_serving_batch_size_bucket[5m]))",
                    "legendFormat": "Median Batch Size"
                },
                {
                    "expr": "histogram_quantile(0.95, rate(model_serving_batch_size_bucket[5m]))",
                    "legendFormat": "P95 Batch Size"
                }
            ]
        },
        {
            "title": "Error Rate",
            "type": "stat",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 12,
                "y": 8
            },
            "targets": [
                {
                    "expr": "sum(rate(model_serving_requests_total{status='error'}[5m])) / sum(rate(model_serving_requests_total[5m])) * 100",
                    "legendFormat": "Error %"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 1
                            },
                            {
                                "color": "red",
                                "value": 5
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "Model Load Time",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 6,
                "x": 18,
                "y": 8
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, rate(model_serving_model_load_time_seconds_bucket[5m]))",
                    "legendFormat": "{{model}} - {{format}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            }
        },
        {
            "title": "Requests by Model",
            "type": "piechart",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 16
            },
            "targets": [
                {
                    "expr": "sum by (model) (increase(model_serving_requests_total[1h]))",
                    "legendFormat": "{{model}}"
                }
            ]
        },
        {
            "title": "GPU Utilization",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 16
            },
            "targets": [
                {
                    "expr": "model_serving_gpu_utilization",
                    "legendFormat": "GPU {{device}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "max": 100,
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 80
                            },
                            {
                                "color": "red",
                                "value": 95
                            }
                        ]
                    }
                }
            }
        }
    ],
    "schemaVersion": 39,
    "tags": [
        "model-serving",
        "ml",
        "inference"
    ],
    "templating": {
        "list": []
    },
    "time": {
        "from": "now-1h",
        "to": "now"
    },
    "timepicker": {},
    "timezone": "browser",
    "title": "Model Serving Overview",
    "uid": "model-serving-overview",
    "version": 1
}