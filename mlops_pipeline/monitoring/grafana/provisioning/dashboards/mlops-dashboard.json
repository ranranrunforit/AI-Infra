{
  "dashboard": {
    "title": "MLOps Pipeline Dashboard",
    "tags": ["mlops", "machine-learning", "model-serving"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 2,
    "refresh": "30s",
    "uid": "mlops-dashboard",
    "editable": true,

    "panels": [
      {
        "id": 1,
        "title": "Prediction Rate (requests/sec)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(model_predictions_total{status=\"success\"}[5m])",
            "legendFormat": "{{model_name}} v{{model_version}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Predictions/sec"},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "pointradius": 5
      },

      {
        "id": 2,
        "title": "Prediction Latency (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(model_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(model_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(model_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 0,
        "linewidth": 2
      },

      {
        "id": 3,
        "title": "Total Predictions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(model_predictions_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },

      {
        "id": 4,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 4, "y": 8},
        "targets": [
          {
            "expr": "rate(model_prediction_errors_total[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.1, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      },

      {
        "id": 5,
        "title": "Model Loaded",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 8, "y": 8},
        "targets": [
          {
            "expr": "model_loaded",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "LOADED"},
              {"type": "value", "value": "0", "text": "NOT LOADED"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },

      {
        "id": 6,
        "title": "Model Accuracy (Training Pipeline)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "model_accuracy",
            "legendFormat": "{{model_name}} v{{model_version}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "min": 0, "max": 1, "label": "Accuracy"},
          {"format": "short"}
        ]
      },

      {
        "id": 7,
        "title": "Model F1 Score (Training Pipeline)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "model_f1_score",
            "legendFormat": "{{model_name}} v{{model_version}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "min": 0, "max": 1, "label": "F1 Score"},
          {"format": "short"}
        ]
      },

      {
        "id": 8,
        "title": "Pipeline Runs",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(pipeline_runs_total{status=\"success\"}[1h])",
            "legendFormat": "Success - {{pipeline_name}}",
            "refId": "A"
          },
          {
            "expr": "rate(pipeline_runs_total{status=\"failed\"}[1h])",
            "legendFormat": "Failed - {{pipeline_name}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Runs/hour"},
          {"format": "short"}
        ]
      },

      {
        "id": 9,
        "title": "Feature Drift Scores",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "feature_drift_score",
            "legendFormat": "{{feature_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "min": 0, "max": 1, "label": "Drift Score"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.1], "type": "gt"},
              "query": {"params": ["A", "5m", "now"]}
            }
          ],
          "name": "High Feature Drift"
        }
      },

      {
        "id": 10,
        "title": "CPU Usage by Pod",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{namespace=\"ml-serving\",pod=~\"churn-model.*\"}[5m])",
            "legendFormat": "{{pod}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "CPU"},
          {"format": "short"}
        ]
      },

      {
        "id": 11,
        "title": "Memory Usage by Pod",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "container_memory_usage_bytes{namespace=\"ml-serving\",pod=~\"churn-model.*\"}",
            "legendFormat": "{{pod}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Memory"},
          {"format": "short"}
        ]
      },

      {
        "id": 12,
        "title": "Active Replicas",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "kube_deployment_status_replicas_available{deployment=\"churn-model\",namespace=\"ml-serving\"}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },

      {
        "id": 13,
        "title": "Deployment Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 36},
        "targets": [
          {
            "expr": "kube_deployment_status_condition{deployment=\"churn-model\",namespace=\"ml-serving\",condition=\"Available\"}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "AVAILABLE"},
              {"type": "value", "value": "0", "text": "UNAVAILABLE"}
            ],
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      }
    ],

    "templating": {
      "list": [
        {
          "name": "namespace",
          "type": "constant",
          "current": {
            "text": "ml-serving",
            "value": "ml-serving"
          },
          "query": "ml-serving"
        }
      ]
    },

    "time": {
      "from": "now-6h",
      "to": "now"
    },

    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
